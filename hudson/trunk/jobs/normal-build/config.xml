<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Do a &quot;normal&quot; build from SVN. Do more testing than the matrix builds.</description>
  <logRotator>
    <daysToKeep>-1</daysToKeep>
    <numToKeep>2</numToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.plugins.javanet.StatsProperty/>
  </properties>
  <scm class="hudson.scm.SubversionSCM">
    <locations>
      <hudson.scm.SubversionSCM_-ModuleLocation>
        <remote>https://squawk.dev.java.net/svn/squawk/trunk</remote>
        <local>trunk</local>
      </hudson.scm.SubversionSCM_-ModuleLocation>
    </locations>
    <useUpdate>true</useUpdate>
    <browser class="hudson.scm.browsers.FishEyeSVN">
      <url>http://fisheye4.atlassian.com/browse/squawk/</url>
      <rootModule></rootModule>
    </browser>
    <excludedRegions></excludedRegions>
    <excludedUsers></excludedUsers>
    <excludedRevprop></excludedRevprop>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <jdk>1.6</jdk>
  <triggers class="vector">
    <hudson.triggers.SCMTrigger>
      <spec>5 * * * *
</spec>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>LD_LIBRARY_PATH=/usr/jdk/instances/jdk1.6.0/jre/lib/i386/client:/usr/jdk/instances/jdk1.6.0/jre/lib/i386:$LD_LIBRARY_PATH
cd trunk

$HUDSON_HOME/bin/verifyBuildJars2

java -jar build.jar clean
java -jar build.jar copyphoneme
java -jar build.jar
java -jar build.jar -prod -mac -o2 rom -metadata cldc imp debugger

ls -l &gt; sizereport.txt

java -jar build.jar map -cp:cldc/j2meclasses:imp/j2meclasses:debugger/j2meclasses squawk.suite
java -jar build.jar genspec
java -jar build.jar jvmenv
java -jar build.jar systemproperties
java -jar build.jar Mnemonics
java -jar build.jar OPC
java -jar build.jar OperandStackEffect
java -jar build.jar Verifier
./squawk -h
./squawk -version
$HUDSON_HOME/bin/runsmoketest.sh DELEGATING

/usr/bin/rm -rf tempdoc

echo MAKEAPI DISABLED FOR NOW!
#java -jar build.jar -verbose makeapi  squawk.suite.api cldc/preprocessed:imp/preprocessed:debugger/preprocessed tempdoc -verbose -docletpath build.jar

java -jar $HUDSON_HOME/findbugs/lib/findbugs.jar -textui -project $HUDSON_HOME/bin/findbugs-bootstrap.fbp -exclude $HUDSON_HOME/bin/findbugs-filter.xml -output findbugs.xml

mkdir tmp
cp squawk squawk.suite squawk.suite.metadata tmp
java -jar build.jar clean 
mv tmp/squawk tmp/squawk.suite tmp/squawk.suite.metadata .
rmdir tmp
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>trunk/squawk,trunk/squawk.suite,trunk/squawk.suite.metadata,trunk/sizereport.txt,trunk/squawk.suite.map</artifacts>
      <latestOnly>false</latestOnly>
    </hudson.tasks.ArtifactArchiver>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>trunk/squawk,trunk/squawk.suite,trunk/squawk.suite.metadata,trunk/sizereport.txt,trunk/squawk.suite.map</artifacts>
      <latestOnly>false</latestOnly>
    </hudson.tasks.ArtifactArchiver>
    <hudson.tasks.BuildTrigger>
      <childProjects>full-debug-build, small-build, build-matrix</childProjects>
      <threshold>
        <name>SUCCESS</name>
        <ordinal>0</ordinal>
        <color>BLUE</color>
      </threshold>
    </hudson.tasks.BuildTrigger>
    <hudson.plugins.findbugs.FindBugsPublisher>
      <threshold></threshold>
      <newThreshold></newThreshold>
      <failureThreshold></failureThreshold>
      <newFailureThreshold></newFailureThreshold>
      <healthy></healthy>
      <unHealthy></unHealthy>
      <pluginName>[FINDBUGS] </pluginName>
      <thresholdLimit>low</thresholdLimit>
      <defaultEncoding></defaultEncoding>
      <pattern></pattern>
    </hudson.plugins.findbugs.FindBugsPublisher>
    <hudson.plugins.tasks.TasksPublisher>
      <threshold></threshold>
      <newThreshold></newThreshold>
      <failureThreshold></failureThreshold>
      <newFailureThreshold></newFailureThreshold>
      <healthy></healthy>
      <unHealthy></unHealthy>
      <pluginName>[TASKS] </pluginName>
      <thresholdLimit>low</thresholdLimit>
      <defaultEncoding></defaultEncoding>
      <high>FIXME, BUG</high>
      <normal>TODO</normal>
      <low></low>
      <ignoreCase>true</ignoreCase>
      <pattern></pattern>
      <excludePattern></excludePattern>
      <canRunOnFailed>false</canRunOnFailed>
    </hudson.plugins.tasks.TasksPublisher>
    <hudson.tasks.Mailer>
      <recipients>squawk-regression@sun.com</recipients>
      <dontNotifyEveryUnstableBuild>false</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>false</sendToIndividuals>
    </hudson.tasks.Mailer>
  </publishers>
  <buildWrappers/>
</project>